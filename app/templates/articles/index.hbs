<table class="primary">
  <thead>
    <tr>
      <th>Description</th>
      <th>Notes</th>
      <th>Borrowed since</th>
      <th>
        <label>
          {{input type="checkbox" name="showReturned" checked=showReturned}}
          Show returned items?
        </label>
      </th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    {{#each filteredResults as |article|}}
      {{articles/article-row article=article save="save" articleStates=possibleStates}}
    {{/each}}
  </tbody>
</table>